    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^7.0.0",
    "jest": "^26.0.1",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.3",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
